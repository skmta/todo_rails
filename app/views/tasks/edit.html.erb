<%= @task.id %>
<%= form_with model: @task, url: task_path(@task), local: true do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.label :due_date %>
    <%= form.datetime_select :due_date %>
  </div>
  <%= form.submit %>
<% end %>
<li><%= link_to @task.title, task_path %></li>